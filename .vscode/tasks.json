{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "clean ctp_run",
      "type": "shell",
      "command": "make",
      "args": [
        "clean"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build ctp_run",
      "type": "shell",
      "command": "make",
      "dependsOn": ["clean"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}